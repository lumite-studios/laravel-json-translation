{
    "name": "lumite-studios/laravel-json-translation",
    "description": "JSON versions of the default Laravel translation files.",
    "type": "library",
    "authors": [
        {
            "name": "Lumite Studios",
            "email": "support@lumitestudios.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LumiteStudios\\JSONTranslations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LumiteStudios\\JSONTranslations\\TranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
